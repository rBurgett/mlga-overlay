{
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "quotes": [1, "single", "avoid-escape"],
        "semi": 2,
        "no-console": 0,
        "no-unused-vars": 1
    },
    "plugins": [],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "$": false,
        "swal": false
    }
}